UIStart'].self['BtnXD'].removeSelf();\r\n            });\r\n            lwg.Global._createHint_01(lwg.Enum.HintType.getXD);\r\n        }\r\n        lwg.LocalStorage.addData();\r\n    }\r\n    lwgDisable() {\r\n        lwg.Global._stageClick = true;\r\n        lwg.Global.GoldNumNode.alpha = 1;\r\n        lwg.Global.ExecutionNumNode.alpha = 1;\r\n    }\r\n}\r\n",
