obal._gameLevel;\r\n        }\r\n        lwg.Admin._openScene(sceneName, 1, this.self, f => { });\r\n    }\r\n    BtnGoldAdvUp(event) {\r\n        event.currentTarget.scale(1, 1);\r\n        lwg.Global._createHint(lwg.Enum.HintType.noAdv);\r\n    }\r\n    BtnExAdvUp(event) {\r\n        event.currentTarget.scale(1, 1);\r\n        lwg.Global._createHint(lwg.Enum.HintType.noAdv);\r\n    }\r\n    onDisable() {\r\n    }\r\n}\r\n",
