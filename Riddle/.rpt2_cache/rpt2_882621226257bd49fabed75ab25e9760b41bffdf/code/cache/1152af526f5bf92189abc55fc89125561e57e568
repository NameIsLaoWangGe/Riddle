vel;\r\n        }\r\n        else if (9 < lwg.Global._gameLevel || lwg.Global._gameLevel <= 99) {\r\n            sceneName = 'UIMain_' + lwg.Global._gameLevel;\r\n        }\r\n        lwg.Global._openInterface(sceneName, 1, this.self, f => { });\r\n    }\r\n    btnGetUp(event) {\r\n        event.currentTarget.scale(1, 1);\r\n        let getLebel = this.GetGold.getChildByName('Num');\r\n        lwg.Global._goldNum += Number(getLebel.text);\r\n        lwg.Global._openInterface('UIStart', 2, this.self, null);\r\n    }\r\n    btnSetUP(event) {\r\n        event.currentTarget.scale(1, 1);\r\n        lwg.Global._openInterface('UISet', 2, null, null);\r\n    }\r\n    onDisable() {\r\n    }\r\n}\r\n",
